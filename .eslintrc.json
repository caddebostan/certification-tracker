{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 100
            }
        ],
        "import/prefer-default-export": "off",
        "import/no-named-as-default": "off",
        "no-script-url": "off",
        "jsx-a11y/anchor-is-valid": "off"
    }
}