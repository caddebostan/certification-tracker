{
  "name": "certification-tracker",
  "version": "0.1.0",
  "private": false,
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "axios": "^0.18.0",
    "chartist": "0.11.0",
    "classnames": "2.2.5",
    "history": "^4.7.2",
    "perfect-scrollbar": "1.3.0",
    "prop-types": "^15.6.0",
    "react": "16.3.2",
    "react-chartist": "0.13.1",
    "react-dom": "16.3.2",
    "react-google-maps": "9.4.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-swipeable-views": "0.12.13",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "yarn run lint",
    "coverage": "yarn run test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js",
    "postcoverage": "istanbul check-coverage --statements 1 --functions 1 --branches 1 --lines 1xw",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint 'src/**/*.css, src/**/*.scss'",
    "lint": "yarn run lint:eslint && yarn run lint:stylelint",
    "format": "prettier --print-width=100 --single-quote --trailing-comma all --write \"{src,__tests__}/**/*.js\"",
    "lint-staged": "lint-staged",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "codecov": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "istanbul": "^0.4.5",
    "lint-staged": "^7.1.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "react-test-renderer": "^16.3.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}